<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="MailLoginTest">
    <test name="Login positive test" parallel="classes" thread-count="2">
        <classes>
            <class name="com.epam.selenium.tests.mail.LoginPositiveTest"></class>
        </classes>
    </test>
    <test name="Login with wrong credential test">
        <classes>
            <class name="com.epam.selenium.tests.mail.LoginWithWrongCredentialTest"></class>
        </classes>
    </test>
    <test name="Login with wrong password test">
        <classes>
            <class name="com.epam.selenium.tests.mail.LoginWithWrongPasswordTest"></class>
        </classes>
    </test>
    <test name="Login with wrong password test using data provider">
        <classes>
            <class name="com.epam.selenium.tests.mail.LoginWithWrongPasswordUsingDPTest"></class>
        </classes>
    </test>
</suite>