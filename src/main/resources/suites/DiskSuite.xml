<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="DiskTest">
    <test name="Download file test" parallel="classes" thread-count="2">
        <classes>
            <class name="com.epam.selenium.tests.disk.DownloadFileTest"/>
        </classes>
    </test>
    <test name="Remove file test">
        <classes>
            <class name="com.epam.selenium.tests.disk.RemoveFileTest"/>
        </classes>
    </test>
    <test name="Remove some file test">
        <classes>
            <class name="com.epam.selenium.tests.disk.RemoveSomeFilesTest"/>
        </classes>
    </test>
    <test name="Restore file test">
        <classes>
            <class name="com.epam.selenium.tests.disk.RestoreFileTest"/>
        </classes>
    </test>
    <test name="Upload file test">
        <classes>
            <class name="com.epam.selenium.tests.disk.UploadFileTest"/>
        </classes>
    </test>
</suite>